.create-merge table MigrationConfiguration (SourceTable:string, DestinationTable:string, WatermarkColumn:string, LastRefreshedTime:datetime, LoadType:string, IsActive:bool) with (folder='ExportConfig')
.create-merge table MigrationAuditLog (TableName:string, Timestamp:datetime, Status:string, FailureReason:string, RecordCount:int) with (folder='ExportConfig')
.create-or-alter materialized-view  VwLatestMigrationConfiguration on table MigrationConfiguration { MigrationConfiguration 
    | where IsActive==true
    |extend IngestionTime = ingestion_time()
    |extend LastRefreshedTimeInString= tostring(LastRefreshedTime)
    |summarize arg_max(LastRefreshedTime, *) by SourceTable }
