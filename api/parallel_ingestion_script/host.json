{
  "version": "2.0",
  "functionTimeout": "00:20:00",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "durableTask": {
      "hubName": "TaskHub",
      "storageProvider": {
        "connectionStringName": "AzureWebJobsStorage",
        "type": "AzureStorage"
      }
    }
  }
}